{
    "name": "sandervanhooft/vaporize-spark-mollie",
    "description": "Run Spark for Mollie on Laravel Vapor",
    "keywords": [
        "sandervanhooft",
        "vaporize-spark-mollie",
        "spark",
        "vapor",
        "mollie"
    ],
    "homepage": "https://github.com/sandervanhooft/vaporize-spark-mollie",
    "license": "MIT",
    "authors": [
        {
            "name": "Sander van Hooft",
            "email": "info@sandervanhooft.com",
            "homepage": "https://www.sandervanhooft.com",
            "role": "Developer"
        }
    ],
    "repositories":[
        {
            "type": "vcs",
            "url": "git@github.com:laravel/spark-aurelius-mollie.git"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/spark-aurelius-mollie": "^2.0",
        "mollie/laravel-cashier-mollie": "^1.12",
        "laravel/vapor-core": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SanderVanHooft\\VaporizeSparkMollie\\": "src"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "SanderVanHooft\\VaporizeSparkMollie\\VaporizeSparkMollieServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
